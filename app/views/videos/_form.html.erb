<%= simple_form_for([current_user,@video], :html => {:multipart => true, :class => "form-
inline"}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :file, :label => 'Source' %> 
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
